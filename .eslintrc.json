{
  "root": true,
  "ignorePatterns": ["**/*"],
  "plugins": ["@nx"],
  "extends": [
    "@code-pushup",
    "@code-pushup/eslint-config/typescript",
    "@code-pushup/eslint-config/node",
    "@code-pushup/eslint-config/jest"
  ],
  "settings": {
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true,
        "project": "tsconfig.base.json"
      }
    }
  }
}
