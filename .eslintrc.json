{
  "root": true,
  "ignorePatterns": ["**/*"],
  "plugins": ["@nx"],
  "settings": {
    "import/resolver": {
      "typescript": {
        "alwaysTryTypes": true,
        "project": "tsconfig.base.json"
      }
    }
  },
  "overrides": [
    {
      "files": ["*.ts"],
      "extends": ["@code-pushup"]
    }
  ]
}
