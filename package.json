{
  "name": "push-based-user-flow-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "build": "nx build cli",
    "kill": "npx kill-port 5032",
    "updateSandboxes": "npm run test -- --testNamePattern=cli-setup",
    "reset": "npm run build && npm run updateSandboxes",
    "test": "nx build cli && nx test cli",
    "publish": "nx run cli",
    "link": "nx link cli",
    "lh": "lighthouse https://example.com --budget-path=./budget.json --output=json --output-path=./lh-inc-budget.json",
    "@push-based/user-flow": "npm run build && npx ./dist/packages/cli/src/cli.js",
    "@push-based/user-flow:help": "npm run @push-based/user-flow -- --help",
    "@push-based/user-flow:init": "npm run @push-based/user-flow -- init -v",
    "@push-based/user-flow:assert": "npm run @push-based/user-flow -- assert -v",
    "@push-based/user-flow:collect": "npm run @push-based/user-flow -- collect -v",
    "@push-based/user-flow:sandboxempty": "npm run testIn:empty-sandbox && npm run @push-based/user-flow -- init -v",
    "@push-based/user-flow:sandboxsetup": "npm run testIn:setup-sandbox && npm run @push-based/user-flow --  -p=./.user-flowrc.static-dist.json  -v",
    "@push-based/user-flow:sandboxsetup:budgets": "npm run testIn:setup-sandbox && npm run @push-based/user-flow -- collect -p=./packages/sandbox-setup/.user-flowrc.static-dist.budgets.json -e=true -v",
    "testIn:empty-sandbox": "npm run build && cd packages/sandbox-empty",
    "testIn:setup-sandbox": "npm run build && cd packages/sandbox-setup",
    "@push-based/user-flow@dist": "npm run link && npx --no-install user-flow",
    "ci-test": "npm run @push-based/user-flow@dist -- -p ./packages/user-flow-ci-integration/.user-flowrc.json -v --format md --open false",
    "prepare": "husky install"
  },
  "dependencies": {
    "@puppeteer/replay": "^1.3.1",
    "@types/concurrently": "^7.0.0",
    "@types/puppeteer": "^5.4.7",
    "@types/yargs": "^17.0.9",
    "concurrently": "^7.1.0",
    "enquirer": "^2.3.6",
    "lighthouse": "^9.6.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "puppeteer": "^19.0.0",
    "rxjs": "^7.5.5",
    "ts-node": "^10.6.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-angular": "^16.2.3",
    "@jscutlery/semver": "^2.20.0",
    "@nrwl/cli": "13.9.5",
    "@nrwl/eslint-plugin-nx": "13.9.5",
    "@nrwl/jest": "13.9.5",
    "@nrwl/js": "13.9.5",
    "@nrwl/linter": "13.9.5",
    "@nrwl/nx-cloud": "13.1.6",
    "@nrwl/tao": "^13.9.5",
    "@nrwl/workspace": "13.9.5",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.11.33",
    "@typescript-eslint/eslint-plugin": "~5.10.0",
    "@typescript-eslint/parser": "~5.10.0",
    "cli-prompts-test": "^0.3.0",
    "commitlint": "^16.2.3",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^7.0.0",
    "jest": "27.2.3",
    "kill-port": "^1.6.1",
    "ngx-deploy-npm": "^3.1.4",
    "nx": "13.9.5",
    "rimraf": "^3.0.2",
    "ts-jest": "27.0.5",
    "typescript": "^4.5.5"
  }
}
