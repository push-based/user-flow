{
  "name": "sandbox-empty",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/sandbox-empty/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/sandbox-empty/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/sandbox-empty/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "e2e": {
      "executor": "@nrwl/workspace:run-commands",
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ],
      "options": {
        "commands": [
          {
            "command": "nx test sandbox-empty"
          }
        ]
      }
    }
  },
  "tags": []
}
