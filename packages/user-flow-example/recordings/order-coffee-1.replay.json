{
  "title": "coffee checkout",
  "steps": [
    {
      "type": "setViewport",
      "width": 1920,
      "height": 969,
      "deviceScaleFactor": 1,
      "isMobile": false,
      "hasTouch": false,
      "isLandscape": false
    },
    {
      "type": "navigate",
      "url": "https://coffee-cart.netlify.app/",
      "assertedEvents": [
        {
          "type": "navigation",
          "url": "https://coffee-cart.netlify.app/",
          "title": "Coffee cart"
        }
      ]
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [["aria/Cappucino"], [".cup:nth-child(1)"]],
      "offsetY": 103.6608657836914,
      "offsetX": 133.40185546875
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [["aria/Proceed to checkout"], ["[data-test=checkout]"]],
      "offsetY": 19.796875,
      "offsetX": 53.1875
    },
    {
      "type": "change",
      "value": "nina",
      "selectors": [["#name"]],
      "target": "main"
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [["#email"]],
      "offsetY": 14.234375,
      "offsetX": 36
    },
    {
      "type": "change",
      "value": "nina@gmail.com",
      "selectors": [["#email"]],
      "target": "main"
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [["aria/Submit"], ["#submit-payment"]],
      "offsetY": 19.859375,
      "offsetX": 48.359375
    }
  ]
}
