          "[data-test=checkout]"
        ]
      ],
      "offsetY": 19.796875,
      "offsetX": 53.1875
    },
    {
      "type": "change",
      "value": "nina",
      "selectors": [
        [
          "#name"
        ]
      ],
      "target": "main"
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [
        [
          "#email"
        ]
      ],
      "offsetY": 14.234375,
      "offsetX": 36
    },
    {
      "type": "change",
      "value": "nina@gmail.com",
      "selectors": [
        [
          "#email"
        ]
      ],
      "target": "main"
    },
    { "type": "endTimespan" },
    {
      "type": "snapshot",
      "stepOptions": {
        "stepName": "Order checked out"
      }
    },
    {
      "type": "startTimespan",
      "stepOptions": {
        "stepName": "Submit order"
      }
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [
        [
          "aria/Submit"
        ],
        [
          "#submit-payment"
        ]
      ],
      "offsetY": 19.859375,
      "offsetX": 48.359375
