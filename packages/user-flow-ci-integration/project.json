{
  "name": "user-flow-ci-integration",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/user-flow-ci-integration/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/user-flow-ci-integration/**/*.ts"
        ]
      }
    },
    "ux-e2e": {
      "executor": "@user-flow/plugin-user-flow:user-flow",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "url": "https://coffee-cart.netlify.app/",
        "rcPath": "packages/user-flow-ci-integration/.user-flowrc.json",
        "ufPath": "packages/user-flow-ci-integration/user-flows",
        "outputPath": "dist/user-flow/user-flow-ci-integration",
        "format": [
          "md"
        ]
      }
    }
  },
  "tags": []
}
