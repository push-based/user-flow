{
  "$schema": "http://json-schema.org/schema",
  "$id": "Target",
  "title": "",
  "type": "object",
  "properties": {
    "targetName": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use for your target?"
    },
    "projectName": {
      "type": "string",
      "description": "",
      "x-prompt": "What project would you like to add your target to?"
    },
    "url": {
      "type": "string",
      "description": "Target URL",
      "default": "https://coffee-cart.netlify.app/",
      "x-prompt": "What URL you want to target?"
    }
  },
  "required": [
    "projectName"
  ]
}
