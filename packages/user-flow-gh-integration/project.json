{
  "name": "user-flow-gh-integration",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/user-flow-gh-integration/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/user-flow-gh-integration/**/*.ts"
        ]
      }
    },
    "user-flow": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "build"
      ],
      "options": {
        "commands": [
          {
            "command": "npm run @push-based/user-flow:collect -- --rcPath=packages/user-flow-gh-integration/.user-flowrc.json -v --tsConfigPath=./packages/user-flow-gh-integration/tsconfig.uf.json"
          }
        ]
      }
    }
  },
  "tags": []
}
